<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.ipcablecloud" name="IP Cable Cloud Repo" version="1.2" provider-name="IP Cable Cloud">
	<extension point="xbmc.addon.repository" name="IP Cable Cloud Repository">
		<info compressed="false">http://raw.github.com/IPCC/IP-Cable-Cloud-Kodi17/master/addons.xml</info>
		<checksum>http://raw.github.com/IPCC/IP-Cable-Cloud-Kodi17/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/IPCC/IP-Cable-Cloud-Kodi17/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>IP Cable Cloud Repo Repository</summary>
		<description>U can Download and Install Official Addons of IP Cable Cloud Repo</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.program.ipcablecloudwizard" name="IP Cable Cloud" version="1.0.7" provider-name="IP Cable Cloud">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests"/>
	<import addon="script.module.beautifulsoup4"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
		<summary lang="en">IP Cable Cloud Wizard</summary>
		<description lang="en">Easy Install of IP Cable Cloud Builds and Addons</description>
		<disclaimer lang="en">Note: we are not responsible or have any influence to the content and their availability from Kodi Add-ons</disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.ipcableboxguide" name="IP Cable Cloud Guide" version="1.0.10" provider-name="IP Cable Cloud">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
		<import addon="script.module.simplejson" version="3.3.0"/>
		<import addon="script.module.requests"/>
		<import addon="script.module.beautifulsoup4"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">IP Cable Cloud Guide allows you to combine some of your favourite live TV plugins for use with a fully working EPG</summary>
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
		<assets>
          <icon>resources/icon.png</icon>
          <fanart>resources/fanart.jpg</fanart>
          <screenshot>resources/screenshot001.png</screenshot>
          <screenshot>resources/screenshot002.png</screenshot>
          <screenshot>resources/screenshot003.png</screenshot>
        </assets>
	</extension>
</addon>

<addon id="script.speedtestnet"
       name="ARNU Box Speed Tester"
       version="1.0.0"
       provider-name="ARNU Box / Josh.5">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">ARNU Box Speed Tester</summary>
    <description lang="en">ARNU Box Speed Tester powered by speedtest.net will give you accurate Internet speed/ping test results. \n Brought to you by http://www.arnubox.com</description>
    <platform>all</platform>
  </extension>
</addon>

